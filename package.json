{
    "name": "@projectyoked/expo-media-engine",
    "version": "0.1.3",
    "description": "Expo module for video composition with text/emoji overlays, audio extraction, and waveform generation. Built with Expo Modules API.",
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "license": "MIT",
    "author": "ProjectYoked",
    "homepage": "https://github.com/SirStig/projectyoked-expo-media-engine#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SirStig/projectyoked-expo-media-engine.git"
    },
    "bugs": {
        "url": "https://github.com/SirStig/projectyoked-expo-media-engine/issues"
    },
    "keywords": [
        "expo",
        "expo-module",
        "expo-modules",
        "video",
        "video-composition",
        "video-editing",
        "media",
        "audio",
        "audio-extraction",
        "waveform",
        "text-overlay",
        "emoji-overlay",
        "avfoundation",
        "mediacodec",
        "react-native"
    ],
    "scripts": {
        "lint": "eslint .",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "typecheck": "tsc --noEmit",
        "validate": "npm run lint && npm run typecheck && npm test",
        "prepare": "npm run build",
        "build": "echo \"Native module - no build step required\"",
        "clean": "rm -rf node_modules && rm -rf ios/build && rm -rf android/build && rm -rf android/.gradle"
    },
    "peerDependencies": {
        "expo": ">=49.0.0",
        "expo-modules-core": ">=1.0.0",
        "react": ">=16.13.1",
        "react-native": ">=0.64.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.23.0",
        "@types/jest": "^29.5.12",
        "babel-jest": "^29.7.0",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "typescript": "^5.3.3"
    },
    "files": [
        "src",
        "ios",
        "android",
        "expo-module.config.json",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}